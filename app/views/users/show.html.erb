<p id="notice"><%= notice %></p>

<h1> Usuários <h2>
<table border=1>
    <thead>
        <tr>
            <th> Nome </th>
            <th> Nome de usuário </th>
            <th> Senha </th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
            <tr>
                <td> <%= user.name %> </td>
                <td> <%= user.username %> </td>
                <td> <%= user.password %> </td>
            </tr>
        <% end %>
    </tbody>
</table>
